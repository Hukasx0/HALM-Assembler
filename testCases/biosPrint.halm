-- Simple bios interrupt program that displays "HELLO"

mov ah,0x0e
mov al,'H'
int 0x10

mov ah,0x0e
mov al,'E'
int 0x10

mov ah,0x0e
mov al,'L'
int 0x10

mov ah,0x0e
mov al,'L'
int 0x10

mov ah,0x0e
mov al,'O'
int 0x10

jmp $

addBytes = (times 478 0x0)
addBytes = 0x55,0xaa
doSh = "qemu-system-x86_64 biosPrint.halm.bin"
